<section>
    @foreach (var shoe in Model)
    {
        <article class="shoe-card">
            <h1>@shoe.Id</h1>
            <h1>@shoe.Type</h1>
            <h1>@shoe.Company</h1>
            <h1>@shoe.Model</h1>
            <h1>@shoe.Price</h1>
            <h1>@shoe.Quantity</h1>
            <h1>@shoe.InSale</h1>
            <img src="@shoe.Image" width="300px" height="300px" />
        </article>
    }
</section>
